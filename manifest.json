{
    "manifest_version": 3,
    "name": "DevVault",
    "version": "1.2.0",
    "description": "LocalStorage Manager & API Caller for Developers",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    }
}